{
  "name": "ESPN Fantasy Baseball Plus",
  "version": "0.1.0",
  "description": "more espn fantasy baseball stats",
  "manifest_version": 3,
  "author": "Robert Ceglie",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "popup.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"]
}
